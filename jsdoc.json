{
    "tags": {
        "allowUnknownTags": true
    },
    "opts": {
        "destination": "./docs"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true,
        "default": {
            "outputSourceFiles": true
        },
        "path": "ink-docstrap",
        "theme": "lumen",
        "navType": "inline",
        "linenums": true,
        "dateFormat": "MMMM Do YYYY, h:mm:ss a"
    }
}
